#!/usr/local/bin/python
import os, sys
sys.path.append("../src")

from extDetect import Extentions
from Imgmain import Imgmain
from ImgThread import ImgThread

if __name__ == "__main__":

    option = Extentions(sys.argv)

    option.CanConversion()

    if option.isConver == 0:
        print (option.output)
    elif option.VERSION == Extentions.VERSION_DISPALY:
        print (option.versionDisplay)
    elif option.SELECTED == Extentions.HTML_NAME:
        Imgmain(sys.argv[1],option).imgmain()
    else:
        Imgmain(sys.argv[1],option).imgmain()
